create table Account(
	username varchar(18) ,
	password char(100) not null,
	coins integer default 400,
	email varchar(60) not null,
	color_my_balls char(7) default '#16004d',
	color_ball_to_play char(7) default '#ffcc00' ,
	lineup integer default 1 ,
	primary key(username)
);

create table Field(
	name varchar(20),
	price integer not null,
	reward integer not null,
	image_field blob not null,
	cover_image blob not null,
	primary key(name)
);

create table Match(
	id integer auto_increment,
	date_match date not null,
	time_match time not null,
	result char(3) not null,
	field varchar(20) not null,
	home varchar(18) not null,
	guest varchar(18) not null,
	primary key(id),
	foreign key(field) references Field(name),
	foreign key(home) references Account(username),
	foreign key(guest) references Account(username)
);

create table Type_of_skin(
	id integer auto_increment,
	name varchar(20) not null,
	base boolean default 0,
	primary key(id)
);

create table Skin(
	name varchar(18) not null,
	price integer not null,
	color char(7)  not null,
	type_of_skin integer not null,
	primary key(name),
	foreign key(type_of_skin) references Type_of_skin(id)	
);

create table Friends(
	account varchar(18) not null,
	friend varchar(18) not null,
	primary key(account,friend),
	foreign key(account) references Account(username),
	foreign key(friend) references Account(username)	
);


create table Inventary(
	account varchar(18) not null,
	skin varchar(18) not null,
	primary key(account,skin),
	foreign key(account) references Account(username),
	foreign key(skin) references Skin(name)	 
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
