<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Circle?>

<BorderPane styleClass="pane" stylesheets="@css/stylesheet1.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.control.SkinControllerInventory">
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="167.0" prefWidth="189.0" styleClass="skin">
         <center>
            <Circle fx:id="skin" fill="#ebeef2" onMouseEntered="#onMouseEnteredCircle" onMouseExited="#onMouseExitedCircle" radius="43.0" stroke="BLACK" strokeType="INSIDE" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <Label fx:id="name" alignment="CENTER" prefHeight="26.0" prefWidth="217.0" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="7.0" />
               </BorderPane.margin>
            </Label>
         </top>
         <bottom>
            <Button fx:id="use_button" mnemonicParsing="false" onAction="#onClickUse_button" prefHeight="30.0" prefWidth="67.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
      </BorderPane>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
